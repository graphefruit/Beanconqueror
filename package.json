    "@angular/compiler-cli": "^20.1.3",
    "@angular/language-service": "^20.1.3",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^8.0.0",
    "@ionic/angular-toolkit": "^9.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/lodash": "^4.14.178",
    "@types/node": "^20.12.7",
    "angular-eslint": "^20.7.0",
