    "chart.js": "^4.5.0",
    "community-cordova-plugin-nfc": "^1.4.0",
    "core-js": "^3.44.0",
    "currency-symbol-map": "^5.1.0",
    "html-to-image": "^1.11.13",
    "javascript-color-gradient": "^2.5.0",
    "localforage-cordovasqlitedriver": "^1.8.0",
    "lodash": "^4.17.21",
    "long": "^5.3.2",
    "luxon": "^3.7.1",
