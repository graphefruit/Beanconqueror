    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "lint-staged": "^13.0.3",
    "prettier": "^3.3.3",
    "protractor": "^7.0.0",
    "skwas-cordova-plugin-datetimepicker": "^3.0.2",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-node": "~8.3.0",
    "ts-proto": "^1.122.0",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.50.0",
    "webpack-subresource-integrity": "^1.5.2"
  },
  "overrides": {
    "@babel/preset-env": {
      "comment": "Not pinning this causes transpilation issues where 'this' is accessed before calling the super constructor. See https://github.com/angular/angular-cli/issues/31674",
      "@babel/plugin-transform-class-static-block": "7.27.1"
    },
    "ag-virtual-scroll": {
      "@angular/common": "$@angular/common",
      "@angular/core": "$@angular/core"
    }
  },
  "description": "Beanconqueror",
  "platforms": [
    "android",
    "ios"
  ],
  "prettier": {
    "singleQuote": true,
    "semi": true
  },
  "lint-staged": {
    "*.css": "stylelint --fix",
    "*.{ts,js,css,md}": "prettier --write"
  }
}
